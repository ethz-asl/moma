<?xml version="1.0"?>
<launch>
    <arg name="frame_id" default="world"/>
    <node pkg="moma_ocs2" type="mobile_manipulator_target" name="mobile_manipulator_target" output="screen"
          args="panda"/>

    <node pkg="panda_mpc" type="interactive_target_relay.py" name="interactive_target_ralay">
        <param name="frame_id" value="$(arg frame_id)"/>
        <param name="in_topic" value="mobile_manipulator_mpc_target"/>
        <param name="out_topic" value="/desired_path"/>
    </node>
</launch>
