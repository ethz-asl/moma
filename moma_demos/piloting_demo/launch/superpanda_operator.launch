<?xml version="1.0" ?>
<launch>
  
  <!-- start rviz -->
  <node name="$(anon rviz)" pkg="rviz" type="rviz" args="-d $(find piloting_demo)/config/rviz/operator.rviz" />

  <!-- dashboard -->
  <rosparam command="load" file="$(find piloting_demo)/config/dashboard.yaml"/>
  <node name="moma_dashboard" pkg="rqt_gui" type="rqt_gui" args="--perspective-file $(find piloting_demo)/config/dashboard.perspective">
      <param name="capability_group" value="GUI"/>
  </node>

  <!-- An interactive marker for the mpc controller-->
  <include file="$(find panda_mpc)/launch/interactive_marker.launch">
    <arg name="frame_id" value="tracking_camera_odom"/>
  </include>
</launch>
